
.done . done!
.start starting ...

include stdlib

%i 2
:main

  push .start
  call :print

  push 0
  push %i
  istore

  :loop

    push %i
    iload
    push 1
    add
    dup
    push %i
    istore

    push 10000000
    eq
    skz 5
      push .done
      call :print
      push 10
      out
      halt

    #push 46
    #out

   jump :loop


halt
